{
  "name": "my-dev-blog",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook -s public",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk ./node_modules/.bin/jest --runInBand",
    "test:debug:watch": "node --inspect-brk ./node_modules/.bin/jest --runInBand --watch",
    "test:debug:coverage": "node --inspect-brk ./node_modules/.bin/jest --runInBand --coverage",
    "test:debug:coverage:watch": "node --inspect-brk ./node_modules/.bin/jest --runInBand --coverage --watch",
    "test:debug:coverage:watch:all": "node --inspect-brk ./node_modules/.bin/jest --runInBand --coverage --watchAll",
    "test:debug:coverage:watch:all:verbose": "node --inspect-brk ./node_modules/.bin/jest --runInBand --coverage --watchAll --verbose",
    "test:debug:coverage:watch:all:verbose:forceExit": "node --inspect-brk ./node_modules/.bin/jest --runInBand --coverage --watchAll --verbose --forceExit",
    "test:debug:coverage:watch:all:verbose:forceExit:notify": "node --inspect-brk ./node_modules/.bin/jest --runInBand --coverage --watchAll --verbose --forceExit --notify",
    "test:debug:coverage:watch:all:verbose:forceExit:notify:notifyMode": "node --inspect-brk ./node_modules/.bin/jest --runInBand --coverage --watchAll --verbose --forceExit --notify --notifyMode=always",
    "eslint": "eslint --ext .js,.jsx,.ts,.tsx .",
    "eslint:fix": "eslint --ext .js,.jsx,.ts,.tsx . --fix",
    "eslint:staged": "lint-staged",
    "precommit": "lint-staged",
    "prepush": "npm run test:coverage",
    "sync": "ts-node ./scripts/sync.ts"
  },
  "dependencies": {
    "cross-env": "^7.0.3",
    "eslint": "8.36.0",
    "eslint-config-next": "13.2.4",
    "mysql2": "^3.2.0",
    "next": "^13.2.4",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-scripts": "^5.0.1",
    "sequelize": "^6.30.0",
    "sequelize-typescript": "^2.1.5"
  },
  "devDependencies": {
    "@babel/plugin-transform-modules-commonjs": "^7.21.2",
    "@babel/preset-react": "^7.18.6",
    "@storybook/addon-actions": "^6.5.15",
    "@storybook/addon-essentials": "^6.5.15",
    "@storybook/addon-interactions": "^6.5.15",
    "@storybook/addon-links": "^6.5.15",
    "@storybook/builder-webpack5": "^6.5.15",
    "@storybook/manager-webpack5": "^6.5.15",
    "@storybook/node-logger": "^6.5.15",
    "@storybook/preset-create-react-app": "^4.1.2",
    "@storybook/react": "^6.5.15",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@types/jest": "^29.5.0",
    "@types/mdx": "^2.0.3",
    "@types/node": "^18.15.9",
    "@types/react": "^18.0.29",
    "@types/react-dom": "18.0.11",
    "@types/sequelize": "^4.28.14",
    "@typescript-eslint/eslint-plugin": "^5.48.0",
    "@typescript-eslint/parser": "^5.48.0",
    "autoprefixer": "^10.4.0",
    "babel-jest": "^29.5.0",
    "babel-loader": "^8.2.3",
    "babel-plugin-module-resolver": "^5.0.0",
    "css-loader": "^6.5.1",
    "eslint": "^8.36.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-storybook": "^0.6.11",
    "eslint-plugin-testing-library": "^5.0.0",
    "eslint-plugin-testing-library-react": "^15.1.4",
    "eslint-plugin-unicorn": "^37.0.1",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "jest": "^29.5.0",
    "lint-staged": "^12.1.4",
    "mini-css-extract-plugin": "^2.4.5",
